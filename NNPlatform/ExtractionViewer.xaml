<UserControl x:Class="NNPlatform.ExtractionEdit"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NNPlatform"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Background="White" Focusable="True">
        <TextBlock Focusable="True" x:Name="Canvas" VerticalAlignment="Top" TextWrapping="Wrap" MouseEnter="Canvas_MouseEnter" MouseLeave="Canvas_MouseLeave"/> 
</UserControl>
